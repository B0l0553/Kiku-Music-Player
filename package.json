{
  "name": "kiku_music_player",
  "version": "0.8.0",
  "description": "A local music player made with w/ electron and vanillajs",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "make-win32": "electron-packager . \"kmp\" --platform=win32 --arch=x64",
    "make-linux": "electron-packager . \"kmp\" --platform=linux --arch=x64",
    "make-arm64": "electron-packager . \"kmp\" --platform=linux --arch=arm64",
    "make-win64": "electron-packager . \"kmp\" --out=dist/win --platform=win32 --arch=x64 --app-version=0.8.0 --icon=assets/icons/yu_music.ico"
  },
  "keywords": [
    "music",
    "player",
    "music-player",
    "electron",
    "vanillajs",
    "electron-vanillajs"
  ],
  "author": "B0l0553 (https://github.com/B0l0553)",
  "bugs": {
    "url": "https://github.com/B0l0553/Kiku-Music-Player/issues"
  },
  "license": "ISC",
  "devDependencies": {
    "electron": "^39.2.1"
  },
  "dependencies": {
    "node-id3": "^0.2.3"
  }
}
